<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>employeeMenu</servlet-name>
    <servlet-class>EmployeeMenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>employeeMenu</servlet-name>
    <url-pattern>/employeeMenu</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>adminMenu</servlet-name>
    <servlet-class>AdminMenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminMenu</servlet-name>
    <url-pattern>/adminMenu</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>employeeList</servlet-name>
    <servlet-class>EmployeeListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>employeeList</servlet-name>
    <url-pattern>/employeeList</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>newRequest</servlet-name>
    <servlet-class>NewRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newRequest</servlet-name>
    <url-pattern>/newRequest</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>profile</servlet-name>
    <servlet-class>ProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>profile</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>myRequestList</servlet-name>
    <servlet-class>MyRequestListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myRequestList</servlet-name>
    <url-pattern>/myRequestList</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>requestResolver</servlet-name>
    <servlet-class>RequestResolverServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>requestResolver</servlet-name>
    <url-pattern>/requestResolver</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>requestAdder</servlet-name>
    <servlet-class>RequestAdderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>requestAdder</servlet-name>
    <url-pattern>/requestAdder</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>fullRequestList</servlet-name>
    <servlet-class>FullRequestListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fullRequestList</servlet-name>
    <url-pattern>/fullRequestList</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>profileUpdate</servlet-name>
    <servlet-class>ProfileUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>profileUpdate</servlet-name>
    <url-pattern>/profileUpdate</url-pattern>
  </servlet-mapping>

</web-app>
